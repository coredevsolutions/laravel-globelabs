{
    "name": "coredev/laravel-globelabs",
    "description": "A Laravel package for GlobeLabs API",
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/contracts": "5.5.*|5.6.*",
        "illuminate/support": "5.5.*|5.6.*",
        "php": "^7.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Coredev Solutions",
            "email": "info@coredev.ph"
        }
    ],
    "autoload": {
        "psr-4": {
            "Globe\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Globe\\GlobeServiceProvider"
            ],
            "aliases": {
                "Globe": "Globe\\Globe"
            }
        }
    }
}
